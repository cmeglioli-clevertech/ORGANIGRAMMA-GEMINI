{
  "servers": {
    "proxy": {
      "port": 3001,
      "command": "npm run proxy",
      "startup_timeout": 30
    },
    "frontend": {
      "port": 3000,
      "command": "npm run dev",
      "startup_timeout": 60
    }
  },
  "browser": {
    "url": "http://localhost:3000",
    "window": {
      "width": 1400,
      "height": 900,
      "position_x": 100,
      "position_y": 100
    }
  },
  "logging": {
    "level": "INFO",
    "max_bytes": 5242880,
    "backup_count": 3
  }
}

